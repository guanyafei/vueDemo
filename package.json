{
    "name": "ax",
    "version": "3.5.0",
    "description": "安迅物流商家工作台",
    "author": "安迅",
    "license": "MIT",
    "scripts": {
        "dev": "vue-cli-service serve",
        "build:prod": "vue-cli-service build",
        "build:stage": "vue-cli-service build --mode staging",
        "preview": "node build/index.js --preview",
        "lint": "eslint --ext .js,.vue src"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,vue}": [
            "eslint --fix",
            "git add"
        ]
    },
    "keywords": [
        "vue",
        "element-ui"
    ],
    "repository": {
        "type": "git"
    },
    "dependencies": {
        "axios": "0.21.0",
        "core-js": "3.8.1",
        "element-ui": "^2.15.5",
        "js-beautify": "1.13.0",
        "js-cookie": "2.2.1",
        "jsencrypt": "3.0.0-rc.1",
        "quill": "1.3.7",
        "vue": "2.6.12",
        "vue-router": "3.4.9",
        "vuex": "3.6.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "4.4.6",
        "@vue/cli-plugin-eslint": "4.4.6",
        "@vue/cli-service": "4.4.6",
        "babel-eslint": "10.1.0",
        "chalk": "4.1.0",
        "connect": "3.6.6",
        "eslint": "7.15.0",
        "eslint-plugin-vue": "7.2.0",
        "lint-staged": "10.5.3",
        "runjs": "4.4.2",
        "sass": "1.32.0",
        "sass-loader": "10.1.0",
        "vue-template-compiler": "2.6.12"
    },
    "engines": {
        "node": ">=8.9",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}